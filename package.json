{
	"name": "lancie-frontend-preact",
	"version": "0.0.1",
	"description": "Lancie frontend build with Preact",
	"repository": "git@github.com:AreaFiftyLAN/lancie-frontend-preact.git",
	"author": "Lancie team",
	"license": "GPL-3.0",
	"private": true,
	"sideEffects": [
		"./src/index.tsx"
	],
	"alias": {
		"react": "preact/compat",
		"react-dom/test-utils": "preact/test-utils",
		"react-dom": "preact/compat"
	},
	"browserslist": "> 0.25%, not dead",
	"scripts": {
		"start": "yarn watch",
		"watch": "parcel ./src/index.html",
		"build": "parcel build ./src/index.html",
		"serve": "yarn build && http-server ./dist -p 1234",
		"lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src && stylelint './src/**/*.scss'"
	},
	"dependencies": {
		"preact": "^10.4.8"
	},
	"devDependencies": {
		"@babel/plugin-transform-react-jsx": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/node": "^14.11.1",
		"@types/parcel-env": "^0.0.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"autoprefixer": "^9.8.6",
		"eslint": "^7.9.0",
		"eslint-config-preact": "^1.1.1",
		"eslint-config-prettier": "^6.11.0",
		"http-server": "^0.12.3",
		"parcel": "^2.0.0-beta.1",
		"postcss": "^7.0.34",
		"postcss-modules": "^3.2.2",
		"sass": "^1.26.11",
		"stylelint": "^13.7.1",
		"stylelint-config-sass-guidelines": "^7.1.0",
		"typescript": "^4.0.2"
	}
}
